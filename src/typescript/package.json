{
  "name": "@letta-ai/memory-sdk",
  "version": "0.1.4",
  "description": "TypeScript SDK for using Letta subagents for pluggable memory management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "npm run build && echo 'âœ… All tests passed for npm publish'",
    "test:memory": "npm run build && node dist/tests/memory.test.js",
    "test:formatter": "npm run build && node dist/tests/prompt-formatter.test.js",
    "test:context": "npm run build && node dist/tests/context.test.js",
    "test:jest": "jest",
    "test:all": "npm run test:memory && npm run test:formatter && npm run test:messages",
    "test:messages": "jest tests/test_messages.test.ts",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "letta",
    "memory",
    "ai",
    "chatbot",
    "typescript"
  ],
  "author": "Sarah Wooders <sarah@example.com>",
  "license": "MIT",
  "dependencies": {
    "@letta-ai/letta-client": "^0.1.211"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sarahwooders/learned-context-sdk"
  },
  "publishConfig": {
    "access": "public"
  }
}
